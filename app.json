{
  "expo": {
    "name": "SweatSmart",
    "scheme": "sweatsmart",
    "slug": "sweatsmart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./sweatsmart/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./sweatsmart/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./sweatsmart/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./sweatsmart/assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
